<?xml version="1.0"?>
<tryton>
    <data>
        <record model="ir.ui.view" id="sale_form1">
            <field name="model">sale.sale</field>
            <field name="inherit" ref="sale.sale_view_form"/>
            <field name="name">sale_form</field>
        </record>
           <record model="ir.ui.view" id="sale_incoterm_view_form">
            <field name="model">sale.incoterm</field>
            <field name="type">form</field>
            <field name="name">sale_incoterm_form</field>
        </record>
        <record model="ir.ui.view" id="sale_incoterm_view_tree">
            <field name="model">sale.incoterm</field>
            <field name="type">tree</field>
            <field name="name">sale_incoterm_tree</field>
        </record>
        <record model="ir.action.act_window" id="act_sale_incoterm_form">
            <field name="name">Sale Incoterm</field>
            <field name="res_model">sale.incoterm</field>
        </record>
           <record model="ir.action.act_window.view" id="act_sale_incoterm_form_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="sale_incoterm_view_tree"/>
            <field name="act_window" ref="act_sale_incoterm_form"/>
        </record>
          <record model="ir.action.act_window.view" id="act_sale_incoterm_form_view2">
            <field name="sequence" eval="20"/>
            <field name="view" ref="sale_incoterm_view_form"/>
            <field name="act_window" ref="act_sale_incoterm_form"/>
        </record>
         <menuitem parent="sale.menu_configuration" action="act_sale_incoterm_form"
            id="menu_sale_incoterm_form"/>   
    </data>
</tryton>
